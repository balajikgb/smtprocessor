@{
    if (Context.User.Identity.IsAuthenticated)
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_HomeLayout.cshtml";
    }
    ViewBag.Title = "Registration Complete";
}
<div class="text-center mt-5">
    <h1 class="mb-5 oot-h1-text">You did not granted any rights in OOT</h1>
</div>
@section Scripts
{
    <script>
    $(function () {
        @if (Context.User.Identity.IsAuthenticated)
            {
                <text>hideLoadingSpinner();</text>
            }
        })
    </script>
}