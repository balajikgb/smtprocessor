<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>@ViewData["Title"] - WebApi</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/lib/datatables/css/jquery.dataTables.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/dataTables.bootstrap4.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/fonts/icomoon/style.css" />
    <link rel="stylesheet" href="~/css/style.css">
</head>
<body>
    <header class="oot-homeheader site-navbar js-sticky-header site-navbar-target" role="banner">
        <div class="container-fluid pr-5">
            <div class="row align-items-center">
                <div class="col-8 col-xl-2 home-heder-text">
                   @* @PriceUpdateWebApp.Resources.OrderResx.HomeOrderTracker*@
                </div>
                <nav class="col-12 col-md-10 d-none d-xl-block site-navigation position-relative text-lg-right">
                    <img class="armstronglogo" src="/Armstrong Logo.svg" />
                </nav>
            </div>
        </div>
    </header>
    <div class="hero">
        <partial name="_CookieConsentPartial" />
        <main role="main" class="pb-3 main-body">
            @RenderBody()
        </main>
        <footer class="border-top footer text-muted">
            <div class="container">
                <div class="row">
                    <div class="col-sm-7">
                        <div class="footer-links">
                        </div>
                        <hr class="d-block d-xl-none" />
                    </div>
                    <div class="col-sm-5">
                    </div>
                </div>
                <div class="row d-block">
                    <div class="col-xs-12">
                        <hr>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-7">
                        <div class="footer-links-terms">
                        </div>
                    </div>
                    <div class="col-sm-5">
                        <div class="footer-copyright">

                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/jquery-3.3.1.min.js"></script>
    <script src="~/js/jquery.sticky.js"></script>
    <script src="~/js/main.js"></script>
    <script src="~/lib/datatables/js/jquery.dataTables.js"></script>
    <script src="~/lib/datatables/js/dataTables.bootstrap4.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/datatables/js/buttons/dataTables.buttons.min.js"></script>
    <script src="~/lib/datatables/js/buttons/jszip.min.js"></script>
    <script src="~/lib/datatables/js/buttons/pdfmake.min.js"></script>
    <script src="~/lib/datatables/js/buttons/vfs_fonts.js"></script>
    <script src="~/lib/datatables/js/buttons/buttons.html5.min.js"></script>
    <script src="~/lib/datatables/js/buttons/buttons.print.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!-- <script src="https://kit.fontawesome.com/62677bec4e.js" crossorigin="anonymous"></script>
    <script src="~/js/62677bec4e.js" ></script>  --> 
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>